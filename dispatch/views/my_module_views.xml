<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_res_config_settings_inherit_form" model="ir.ui.view">
        <field name="name">res.config.settings.inherit.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[preceding-sibling::block[position()=2]]" position="inside">
                <setting id="stock_transport" help="Enable Stock Transport module">
                    <field name="_stock_transport"/>
                </setting>
            </xpath>

        </field>
    </record>
</odoo>